<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="com.moma.trip" extends="web-default" namespace="/trip/activity">
		<action name="index" method="index" class="trip.activityPlanAction">
			<result name="success">/views/activity/index.jsp</result>
		</action>
		
		<action name="toQuery" method="toQuery" class="trip.activityPlanAction">
			<result name="success">/views/activity/query.jsp</result>
		</action>
		
		<action name="query" method="query" class="trip.activityPlanAction">
			<result name="success">/views/activity/list.jsp</result>
		</action>
		
		<action name="detail" method="detail" class="trip.activityPlanAction">
			<result name="success">/views/activity/detail.jsp</result>
		</action>
	</package>
</struts>
